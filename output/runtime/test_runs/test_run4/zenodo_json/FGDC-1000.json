  "metadata": {
    "title": "The Effect of Particle Size on the Light Attenuation Coefficient of Natural Suspensions",
    "upload_type": "dataset",
    "publication_date": "1984-01-01",
    "creators": [
      {
        "name": "Oceanic, National"
      },
